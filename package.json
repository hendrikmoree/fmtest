{
    "name": "fmtest",
    "displayName": "fmtest",
    "description": "fmtest only switcher",
    "version": "0.0.1",
    "publisher": "hendrikmoree",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.toggleOnly",
                "title": "Toggle .only"
            }
        ],
	"menus": {
            "touchBar": [{"command": "extension.toggleOnly"}]
        },
        "snippets": [{
            "language": "javascript",
            "path": "./snippets.json"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}
